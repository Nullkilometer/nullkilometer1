<% if admin_signed_in? %>
  <%= link_to(I18n.t("devise.commands.logout"), destroy_admin_session_path, :method => :delete, class: "ordinaryLink") %>/
  <%= link_to(I18n.t("devise.commands.edit_account"), edit_admin_registration_path, class: "ordinaryLink") %>
<% else %>
  <%= link_to(I18n.t("devise.commands.login"), new_admin_session_path, class: "ordinaryLink")  %>/<%= link_to(I18n.t("devise.commands.register"), new_admin_registration_path, class: "ordinaryLink")  %>
<% end %>
