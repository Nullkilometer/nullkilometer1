<!-- Modal bootstrap element-->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only"><%= I18n.t("button.close") %></span></button>
        <h4 class="modal-title" id="myModalLabel"><%= I18n.t("headers.address_search_results") %></h4>
      </div>
      <div class="modal-body" id="locationResultList"></div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal"><%= I18n.t("button.close") %></button>
      </div>
    </div>
  </div>
</div>